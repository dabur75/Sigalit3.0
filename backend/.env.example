# Production Environment Variables for Railway
NODE_ENV=production
PORT=4000

# PostgreSQL Database (Railway will provide DATABASE_URL)
DATABASE_URL=postgresql://user:password@host:port/database

# Optional: Frontend serving mode
SERVE_FROM_FRONTEND=false

# AI Agent settings (optional)
AI_AGENT_ENABLED=true

# Add your LLM API keys when ready (from LLM_ENHANCEMENT_PLAN.md)
# CLAUDE_API_KEY=your_claude_api_key
# OPENAI_API_KEY=your_openai_api_key